{
    "name": "gemstracker/gemstracker",
    "description": "GemsTracker",
    "license": "BSD-3-Clause",
    "type": "library",
    "homepage": "https://gemtracker.org",
    "authors": [
        {
            "name": "Matijs de Jong",
            "email": "mjong@magnafacta.nl"
        },
        {
            "name": "Menno Dekker",
            "email": "menno.dekker@erasmusmc.nl"
        },
        {
            "name": "Jasper van Gestel",
            "email": "jvangestel@gmail.com"
        },
        {
            "name": "Frank Santegoets",
            "email": "f.santegoets@erasmusmc.nl"
        },
        {
            "name": "Roel Faber",
            "email": "r.faber@erasmusmc.nl"
        },
        {
            "name": "Michiel Rook",
            "email": "mrook@php.net"
        }
    ],
    "autoload": {
        "classmap": ["classes/GemsEscort.php"],
        "psr-0": {
            "Gems_": "classes",
            "Zend_": "classes",
            "OpenRosa_" : "classes"
        },
        "psr-4": {
            "Gems\\": "classes/Gems"
        }
    },
    "include-path": [
        "classes/"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.0",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "ext-mysqli": "*",
        "ext-soap": "*",
        "ext-xmlreader": "*",
        "ext-xmlwriter": "*",
        "ext-zip": "*",
        "lib-iconv": "*",
        "lib-curl": "*",
        "lib-openssl": "*",
        "lib-libxml": "*",
        "magnafacta/mutil": "~1.8.2",
        "box/spout": "^2.7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phing/phing": "^2.11",
        "phpunit/dbunit": "1.3.*",
        "jokkedk/zfdebug": "1.6.2",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload-dev": {
        "psr-0": {
            "Gems_": "tests/classes",
            "MUtil_": "vendor/magnafacta/mutil/tests/"
        },
        "psr-4": {
            "Gems\\": "tests/classes/Gems",
            "MUtil\\": "vendor/magnafacta/mutil/tests/MUtil"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/GemsTracker/MUtil.git"
        }
    ]

}
